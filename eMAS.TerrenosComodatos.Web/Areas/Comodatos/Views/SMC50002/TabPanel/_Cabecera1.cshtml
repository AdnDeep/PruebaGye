@model eMAS.TerrenosComodatos.Domain.DTOs.TramiteEditViewModel
@{ 
    string fechaInscripcionRegProp = "";
    if (Model != null && Model.fechainsregprop != null)
        fechaInscripcionRegProp = Model.fechainsregprop.Value.ToString("dd/MM/yyyy");
}

<div class="tramite-cabecera-1 tab-content-form">
    <div class="form-group row">
        <div class="col-12 col-sm-12 col-md-6 col-lg-6">
            <label for="anioEdit">A&ntilde;o</label>
            <input type="text" readonly class="form-control" id="anioEdit" value="@Model.anio">
        </div>
        <div class="col-12 col-sm-12 col-md-6 col-lg-6">
            <label for="secuenciaEdit">Secuencia</label>
            <input type="text" readonly class="form-control" id="secuenciaEdit" value="@Model.secuencia">
        </div>
    </div>
    <div class="form-group row">
        <div class="col-12 col-sm-12 col-md-12 col-lg-12">
            <div class="row ctrl-codigo-catastral">
                <div class="col-6 col-sm-6 col-md-6 col-lg-2">
                    <label class="mx-1">Sector</label>
                    <input type="number" min="0" class="form-control mx-1" id="sectorEdit" value="@Model.idsector">
                </div>
                <div class="col-6 col-sm-6 col-md-6 col-lg-2">
                    <label class="mx-1">Manzana</label>
                    <input type="number" min="0" class="form-control mx-1" id="manzanaEdit" value="@Model.manzana">
                </div>
                <div class="col-6 col-sm-6 col-md-6 col-lg-2">
                    <label class="mx-1">Solar</label>
                    <input type="number" min="0" class="form-control mx-1" id="loteEdit" value="@Model.lote">
                </div>
                <div class="col-6 col-sm-6 col-md-6 col-lg-2">
                    <label class="mx-1">Divisi&oacute;n</label>
                    <input type="number" min="0" class="form-control mx-1" id="divisionEdit" value="@Model.division">
                </div>
                <div class="col-6 col-sm-6 col-md-6 col-lg-1">
                    <label class="mx-1">Phv</label>
                    <input type="number" min="0" class="form-control mx-1" id="phvEdit" value="@Model.phv">
                </div>
                <div class="col-6 col-sm-6 col-md-6 col-lg-1">
                    <label class="mx-1">Phh</label>
                    <input type="number" min="0" class="form-control mx-1" id="phhEdit" value="@Model.phh">
                </div>
                <div class="col-12 col-sm-12 col-md-12 col-lg-2">
                    <label class="mx-1">N&uacute;mero</label>
                    <input type="number" min="0" class="form-control mx-1" id="numeroEdit" value="@Model.numero">
                </div>
            </div>
        </div>
    </div>
    <div class="form-group row">
        <div class="col-12 col-sm-12 col-md-12 col-lg-6">
            <label for="beneficiarioEdit">Nombre Beneficiario</label>
            <select id="beneficiarioEdit" class="form-control ctrl-select">
            </select>
        </div>
        <div class="col-12 col-sm-12 col-md-6 col-lg-6">
            <label for="representanteLegalEdit">Representante Legal</label>
            <input type="text" readonly class="form-control" id="representanteLegalEdit">
        </div>
    </div>
    <div class="form-group row">
        <div class="col-12 col-sm-12 col-md-6 col-lg-6">
            <label for="oficioAgEdit">Oficio Ag</label>
            <input type="text" class="form-control" id="oficioAgEdit">
        </div>
        <div class="col-12 col-sm-12 col-md-6 col-lg-6">
            <label for="oficioDaseEdit">Oficio Dase</label>
            <input type="text" class="form-control" id="oficioDaseEdit">
        </div>
    </div>
    <div class="form-group row">
        <div class="col-12 col-sm-12 col-md-6 col-lg-6">
            <label for="solar-edit">&Aacute;rea</label>
            <input type="text" class="form-control" id="solar-edit" value="@Model.areasolar">
        </div>
        <div class="col-12 col-sm-12 col-md-6 col-lg-6" id="fecha-inspeccion-container">
            <label for="fecha-inspeccion-edit">Fecha Inscripci&oacute;n Reg. Propiedad</label>
            <input id="fecha-inspeccion-edit"
                   data-date-container='#fecha-inspeccion-container'
                   class="form-control" value="@fechaInscripcionRegProp" />
        </div>
    </div>
    
</div>
