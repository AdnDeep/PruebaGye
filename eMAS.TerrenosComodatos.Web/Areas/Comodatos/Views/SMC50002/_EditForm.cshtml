@model eMAS.TerrenosComodatos.Domain.DTOs.TramiteEditViewModel

@{ 
    eMAS.TerrenosComodatos.Web.Models.DetailViewModel anexoDetailViewModel = new eMAS.TerrenosComodatos.Web.Models.DetailViewModel();
    anexoDetailViewModel.addColumnInfo(new eMAS.TerrenosComodatos.Web.Models.ColumnViewModel { id = "acciones", name = "Acciones", visible=true });
    anexoDetailViewModel.addColumnInfo(new eMAS.TerrenosComodatos.Web.Models.ColumnViewModel { id = "enlace", name = "Enlace", visible = true });
    anexoDetailViewModel.addColumnInfo(new eMAS.TerrenosComodatos.Web.Models.ColumnViewModel { id = "id", name = "id", visible = false });

    eMAS.TerrenosComodatos.Web.Models.DetailViewModel observacionDetailViewModel = new eMAS.TerrenosComodatos.Web.Models.DetailViewModel();
    observacionDetailViewModel.addColumnInfo(new eMAS.TerrenosComodatos.Web.Models.ColumnViewModel { id = "acciones", name = "Acciones", visible = true });
    observacionDetailViewModel.addColumnInfo(new eMAS.TerrenosComodatos.Web.Models.ColumnViewModel { id = "fecha", name = "Fecha", visible = true });
    observacionDetailViewModel.addColumnInfo(new eMAS.TerrenosComodatos.Web.Models.ColumnViewModel { id = "observacion", name = "observacion", visible = true });
    observacionDetailViewModel.addColumnInfo(new eMAS.TerrenosComodatos.Web.Models.ColumnViewModel { id = "id", name = "id", visible = false });
}
<div class="form-content-edit">
    <div class="container-form">
        <input id="IdTramite" class="idelemento" type="hidden" value="@Model.idtramite" />
        <input id="IdBeneficiario" class="idelemento" type="hidden" value="@Model.idbeneficiario" />
        <input id="IdTipoContrato" class="idelemento" type="hidden" value="@Model.idtipocontrato" />
        @await Html.PartialAsync("_CommandTop")


        <div id="TramiteTabContainer">
            <ul class="nav nav-tabs" id="tramiteTab" role="tablist">
                <li class="nav-item" role="presentation">
                    <a class="nav-link active" id="cabecera-1-tab" data-toggle="tab" href="#cabecera-1" role="tab" aria-controls="cabecera-1" aria-selected="true">General</a>
                </li>
                @if (Model.idtramite > 0)
                {
                    <li class="nav-item" role="presentation">
                        <a class="nav-link" id="seguimiento-tab" data-toggle="tab" href="#seguimiento" role="tab" aria-controls="seguimiento" aria-selected="false">Seguimientos</a>
                    </li>
                    <li class="nav-item" role="presentation">
                        <a class="nav-link" id="cabecera-2-tab" data-toggle="tab" href="#cabecera-2" role="tab" aria-controls="cabecera-2" aria-selected="false">Datos</a>
                    </li>
                    <li class="nav-item" role="presentation">
                        <a class="nav-link" id="anexo-tab" data-toggle="tab" href="#anexo" role="tab" aria-controls="anexo" aria-selected="false">Anexos</a>
                    </li>
                    <li class="nav-item" role="presentation">
                        <a class="nav-link" id="observacion-tab" data-toggle="tab" href="#observacion" role="tab" aria-controls="observacion" aria-selected="false">Observaciones</a>
                    </li>
                }
            </ul>
            <div class="tab-content" id="tramiteTabContent">
                <div class="tab-pane fade show active" id="cabecera-1" role="tabpanel" aria-labelledby="cabecera-1-tab">
                    @await Html.PartialAsync("TabPanel/_Cabecera1", Model)
                </div>
                @if (Model.idtramite > 0)
                {
                    <div class="tab-pane fade" id="seguimiento" role="tabpanel" aria-labelledby="seguimiento-tab">
                        <p>
                            Placeholder content for the tab panel. This one relates to the profile tab. You got the finest architecture. Passport stamps, she's cosmopolitan. Fine, fresh, fierce, we got it on lock. Never planned that one day I'd be losing you. She eats your heart out. Your kiss is cosmic, every move is magic. I mean the ones, I mean like she's the one. Greetings loved ones let's take a journey. Just own the night like the 4th of July! But you'd rather get wasted.
                        </p>
                    </div>
                    <div class="tab-pane fade" id="cabecera-2" role="tabpanel" aria-labelledby="cabecera-2-tab">
                        @await Html.PartialAsync("TabPanel/_Cabecera2", Model)
                    </div>
                    <div class="tab-pane fade" id="anexo" role="tabpanel" aria-labelledby="anexo-tab">
                        <div class="container-anexo-detail">
                            @await Html.PartialAsync("TabPanel/_Detail", anexoDetailViewModel)
                        </div>
                    </div>
                    <div class="tab-pane fade" id="observacion" role="tabpanel" aria-labelledby="observacion-tab">
                        <div class="container-observacion-detail">
                            @await Html.PartialAsync("TabPanel/_Detail", observacionDetailViewModel)
                        </div>
                    </div>
                }
            </div>
        </div>
        @await Html.PartialAsync("_CommandBottom")
    </div>
</div>
